{
  "type": "object",
  "properties": {
    "message": {
      "type": "string"
    },
    "summary_html": {
      "type": "string"
    },
    "priority_level": {
      "type": "string",
      "enum": ["high", "medium", "low"]
    },
    "estimated_effort": {
      "type": "string",
      "enum": ["small", "medium", "large"]
    },
    "next_action": {
      "type": "string",
      "enum": ["proceed_to_report"]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "word_count": {
          "type": "number"
        },
        "generated_at": {
          "type": "string"
        },
        "tracking_id": {
          "type": "string"
        }
      },
      "required": ["word_count", "generated_at", "tracking_id"]
    },
    "reasoning": {
      "type": "string"
    }
  },
  "required": ["message", "summary_html", "priority_level", "estimated_effort", "next_action", "metadata", "reasoning"]
}
